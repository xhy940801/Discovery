<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<include file="struts-default.xml"/>
	<constant name="struts.objectFactory" value="spring" />
	<package name="discovery" namespace="/" extends="struts-default">
		<action name="login" class="userAction" method="login">
			<result name="success">/WEB-INF/JSP/userSecuInfo.jsp</result>
		</action>
		<action name="getEsseInfo" class="userAction" method="getEsseInfo">
			<result name="success">/WEB-INF/JSP/userEsseInfo.jsp</result>
		</action>
	</package>
</struts>
